<%= form_with model: [ @user, @user.degrees.build ] do |form| %>
  <p>
    <%= form.label :"School Name" %><br>
    <%= form.text_field :schoolname %>
  </p>
  <p>
    <%= form.label :"Start Date" %><br>
    <%= form.date_field :sdate %>
  </p>
  <p>
    <%= form.label :"End Date" %><br>
    <%= form.date_field :edate %>
  </p>
  <p>
    <%= form.label :"Education Level" %><br>
    <%= form.text_field :educationlevel %>
  </p>
  <p>
    <%= form.label :status %><br>
    <%= form.select :status, Visible::VALID_STATUSES, selected 'public'%>
  <p>
    <%= form.submit %>
  </p>
<% end %>