<h1>Users</h1>

<ul>
    <% @users.each do |user| %>
        <% unless user.archived? %>
            <li>
                <%= link_to user.fname user.lname, User %>
            </li>
        <% end %>
    <% end %>
</ul>

<%= link_to "New User", new_user_path %>
<%= link_to "Already am a User", new_user_session_path %>
